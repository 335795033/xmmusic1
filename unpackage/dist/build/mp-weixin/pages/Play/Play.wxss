.imt-audio {
  position: absolute;
  bottom: 0;
  width: 96%;
  /* padding: 0 5% 2rem; */
  background: none;
  margin: 0 auto;
}
@font-face {
  font-family: 'iconfont';
  src: url(data:font/ttf;base64,);
}
.test {
  font-family: iconfont;
}
.bg {
  position: absolute;
  left: 0;
  top: 0;
  bottom: 0;
  right: 0;
  z-index: -1;
  opacity: .6;
  -webkit-filter: blur(10px);
          filter: blur(10px);
}
.top {
  position: relative;
}
.top .back {
    position: absolute;
    top: .5rem;
    left: .5rem;
    z-index: 9;
}
.top .back .test {
      font-size: 1.5rem;
      font-weight: bold;
}
.top .songname {
    width: 100%;
    text-align: center;
    padding: 1rem 0 .5rem 0;
    font-size: 1rem;
    color: #000;
}
.top .name {
    width: 100%;
    text-align: center;
    font-size: .5rem;
    color: #666;
}
.middle {
  margin: 2rem 0 0;
  width: 100%;
  height: 40%;
}
.middle .middle_l {
    width: 80%;
    margin: 0 auto;
}
.middle .middle_l .cd-wrapper {
      border: 0.5rem solid rgba(255, 255, 255, 0.3);
}
.middle .middle_l .cd-wrapper .cd {
        width: 100%;
}
.middle .middle_l .cd-wrapper .cd image {
          width: 100%;
}
.middle .lyric-wrapper {
    width: 80%;
    margin: 2rem auto 0;
}
.middle .lyric-wrapper .lyric {
      width: 100%;
      text-align: center;
      font-size: .8rem;
      color: #666;
}
.bottom {
  position: absolute;
  bottom: 0;
  width: 100%;
}
.bottom .progress-wrapper {
    width: 80%;
    margin: 0 auto;
    display: -webkit-box;
    display: -webkit-flex;
    display: -ms-flexbox;
    display: flex;
}
.bottom .progress-wrapper .progress {
      -webkit-box-flex: 1;
      -webkit-flex: 1;
          -ms-flex: 1;
              flex: 1;
}
.icon {
  display: -webkit-box;
  display: -webkit-flex;
  display: -ms-flexbox;
  display: flex;
  -webkit-justify-content: space-around;
      -ms-flex-pack: distribute;
          justify-content: space-around;
  margin: 1rem auto 2rem;
}
.icon .test {
    font-size: 2rem;
}

